<h1>Entries#new</h1>
<p>Find me in app/views/entries/new.html.erb</p>
<!-- creates a form that saves the results onto the symbol, entries-->
<%= form_for :entries do |f| %> 
	<p>
		<%= f.label :title %><br>
		<%= f.text_field :title %>
	</p>

	<p>
		<%= f.label :text %><br>
		<%= f.text_area :text %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>
